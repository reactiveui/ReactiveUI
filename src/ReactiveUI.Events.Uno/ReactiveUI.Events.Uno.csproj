<Project Sdk="MSBuild.Sdk.Extras">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;Xamarin.iOS10;Xamarin.Mac20;MonoAndroid81;MonoAndroid90</TargetFrameworks>
    <RootNamespace>ReactiveUI.Events</RootNamespace>
    <Description>Provides Observable-based events API for Uno UI controls/eventhandlers. The contents of this package is automatically generated, please target pull-requests to the code generator.</Description>
    <PackageId>ReactiveUI.Events.Uno</PackageId>
    <NoWarn>$(NoWarn);CS1570;CA1812</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Uno.UI" Version="1.44.1" />
    <PackageReference Include="Uno.Core" Version="1.27.0" />
    <PackageReference Include="Pharmacist.MsBuild" Version="1.*" PrivateAssets="all" />
    <PackageReference Include="Pharmacist.Common" Version="1.*" />
   </ItemGroup>
</Project>
