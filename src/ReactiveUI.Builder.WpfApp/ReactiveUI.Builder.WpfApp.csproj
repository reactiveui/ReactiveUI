<Project Sdk="Microsoft.NET.Sdk" Condition="'$(IsWindows)' == 'true'">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>$(ReactiveUIWindowsTargets)</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <IsPackable>false</IsPackable>
    <DefaultPackageDescription>A sample WPF application using ReactiveUI.</DefaultPackageDescription>
    <IsAotCompatible>false</IsAotCompatible>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ReactiveUI.Wpf\ReactiveUI.Wpf.csproj" />
  </ItemGroup>

</Project>
