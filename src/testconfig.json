{
  "platform": {
    "execution": {
      "parallel": false
    }
  },
  "extensions": [
    {
      "extensionId": "Microsoft.Testing.Extensions.CodeCoverage",
      "settings": {
        "format": "cobertura",
        "skipAutoProperties": true,
        "modulePaths": {
          "include": [ "ReactiveUI\\..*" ],
          "exclude": [ ".*Tests.*", ".*TestRunner.*" ]
        }
      }
    }
  ]
}
